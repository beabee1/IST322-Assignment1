<script src="https://cdn.rebuyengine.com/onsite/js/rebuy.js?shop=raycon.myshopify.com" async></script>
<script>
  var getURL = function (variantName) {
    var url = 'https://cdn.shopify.com/s/files/1/2404/0041/t/221/assets/'
    url += 'swatch-' + variantName.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-$/, '').replace(/^-/, '');
    
    url = url.replaceAll("'", '')
    url = url.replaceAll("&", '')
    url = url.replaceAll("®", '')
    url = url.replaceAll('  ', '-');
    url = url.replaceAll(' ', '-');
    url += '_50x50.png'
    url = url.toLowerCase();
    return url;
  }
</script>


